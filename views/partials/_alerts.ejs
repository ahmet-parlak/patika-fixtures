<% if(flashMessages.errors){ %>
<!-- Error Alerts -->
<div class="error-alerts mt-3 text-center">
  <div class="alert alert-danger" role="alert">
    <ul>
      <% flashMessages.errors.forEach(message => {
          %>

      <li> <%- message %> </li>

      <% }); %>
    </ul>
  </div>
</div>
<% } %>




<% if(flashMessages.success){ %>
<!-- Success Alerts -->
<div class="success-alerts mt-3 text-center">
  <div class="alert alert-success" role="alert">
    <ul>
      <% flashMessages.success.forEach(message => {
        %>

      <li> <%- message %> </li>

      <% }); %>
    </ul>
  </div>
</div>
<% } %>